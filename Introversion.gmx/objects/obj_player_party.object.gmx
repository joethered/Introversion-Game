    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(!hasLost){
    if(!try_convo &amp;&amp; !greeting){
            if (keyboard_check(vk_right)){
            hspeed = 1.5
            image_speed = .2;
            facing = 1;
            hasCollided = false;
            
        }
        if (hspeed &gt; 0 &amp;&amp; keyboard_check_released(vk_right)){
            hspeed = 0;
        }
        
        if (keyboard_check(vk_left)){
            hspeed = -1.5;
            image_speed = .2;
            facing = -1;
            hasCollided = false;
        }
        if (hspeed &lt; 0 &amp;&amp; keyboard_check_released(vk_left)){
            hspeed = 0;
        }
        
        
        
        if (keyboard_check(vk_down)){
            vspeed = 1;
            image_speed = .2;
            hasCollided = false;
        }
        
        if (vspeed &gt; 0 &amp;&amp; keyboard_check_released(vk_down)){
            vspeed = 0;
        }
        
        if (keyboard_check(vk_up)){
            vspeed = -1;
            image_speed = .2;
            hasCollided = false;
        }
        if (vspeed &lt; 0 &amp;&amp; keyboard_check_released(vk_up)){
            vspeed = 0;
        }
        
        if(conversable &amp;&amp; keyboard_check(ord('X')) &amp;&amp; hasCollided)
        {
            try_convo = true;
        }
    }else if (!greeting){
        convo_courage -= 1;
        if(keyboard_check_pressed(ord('X')))
        convo_courage += 10;{
            if (convo_courage &gt;= 100){
            audio_play_sound(sfx_gain,true,false);
                greeting = true;
                alarm[1] = 30;
            }  
            if (convo_courage &lt; 0){
                try_convo = false;
                convo_courage = 50;
                conversable =false
                alarm[0] = 30;
            }
        }
    }
    depth = -y +100;
}
else
{
    if(zoom_factor &gt; 0.01){
        zoom_factor -= 0.03;
    }
    else
    {
        zoom_factor = 0.01;
    }
    
    view_wview[0] = 180 * zoom_factor;
    view_hview[0] = 128 * zoom_factor;
}


